<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!--Product-->
        <record id="hos_product_sequence" model="ir.sequence">
            <field name="name">Hospital Product</field>
            <field name="code">hos.product</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!--Stores-->
        <record id="stock_adjustment_sequence" model="ir.sequence">
            <field name="name">Stock Adjustment</field>
            <field name="code">picking.sa</field>
            <field name="prefix">SA/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="store_request_sequence" model="ir.sequence">
            <field name="name">Store Request</field>
            <field name="code">store.request</field>
            <field name="prefix">SR/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="store_issue_sequence" model="ir.sequence">
            <field name="name">Store Issue</field>
            <field name="code">picking.si</field>
            <field name="prefix">SI/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="store_return_sequence" model="ir.sequence">
            <field name="name">Store Return</field>
            <field name="code">store.return</field>
            <field name="prefix">SRN/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="store_intake_sequence" model="ir.sequence">
            <field name="name">Store Intake</field>
            <field name="code">picking.sin</field>
            <field name="prefix">SIN/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="hos_move_in_sequence" model="ir.sequence">
            <field name="name">Product IN</field>
            <field name="code">hos.move.in</field>
            <field name="prefix">IN/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="hos_move_internal_sequence" model="ir.sequence">
            <field name="name">Product Internal</field>
            <field name="code">hos.move.internal</field>
            <field name="prefix">INT/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="hos_move_out_sequence" model="ir.sequence">
            <field name="name">Product OUT</field>
            <field name="code">hos.move.out</field>
            <field name="prefix">OUT/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>


</odoo>