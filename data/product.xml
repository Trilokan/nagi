<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!--Group-->
        <record id="general_group" model="product.group">
            <field name="name">General</field>
            <field name="code">GEN</field>
        </record>

        <!--Sub-Group-->
        <record id="general_sub_group" model="product.sub.group">
            <field name="name">General</field>
            <field name="code">GEN</field>
            <field name="group_id" ref="nagi.general_group"/>
        </record>

        <!--Category-->
        <record id="medicine_category" model="hos.product.category">
            <field name="name">Medicine</field>
            <field name="code">MED</field>
        </record>

        <record id="equipment_category" model="hos.product.category">
            <field name="name">Equipment</field>
            <field name="code">EQU</field>
        </record>

        <record id="service_category" model="hos.product.category">
            <field name="name">Service</field>
            <field name="code">SER</field>
        </record>

        <record id="tax_category" model="hos.product.category">
            <field name="name">Tax</field>
            <field name="code">TAX</field>
        </record>

        <record id="treatment_category" model="hos.product.category">
            <field name="name">Treatment</field>
            <field name="code">TRT</field>
        </record>

        <record id="laboratory_category" model="hos.product.category">
            <field name="name">Laboratory</field>
            <field name="code">LAB</field>
        </record>

        <record id="freight_category" model="hos.product.category">
            <field name="name">Freight</field>
            <field name="code">FRE</field>
        </record>

        <record id="discount_category" model="hos.product.category">
            <field name="name">Discount</field>
            <field name="code">Disc</field>
        </record>

        <record id="miscellaneous_category" model="hos.product.category">
            <field name="name">Miscellaneous</field>
            <field name="code">MISC</field>
        </record>

        <record id="others_category" model="hos.product.category">
            <field name="name">Others</field>
            <field name="code">OTH</field>
        </record>

        <!--Tax-->
        <record id="gst_0_tax" model="hos.tax">
            <field name="name">GST</field>
            <field name="code">GST 0</field>
            <field name="rate">0</field>
        </record>

        <record id="gst_5_tax" model="hos.tax">
            <field name="name">GST</field>
            <field name="code">GST 5</field>
            <field name="rate">5</field>
        </record>

        <record id="gst_12_tax" model="hos.tax">
            <field name="name">GST</field>
            <field name="code">GST 12</field>
            <field name="rate">12</field>
        </record>

        <record id="gst_18_tax" model="hos.tax">
            <field name="name">GST</field>
            <field name="code">GST 18</field>
            <field name="rate">18</field>
        </record>

        <record id="gst_28_tax" model="hos.tax">
            <field name="name">GST</field>
            <field name="code">GST 28</field>
            <field name="rate">28</field>
        </record>


    </data>


</odoo>