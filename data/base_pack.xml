<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_company_sequence" model="ir.sequence">
        <field name="name">Hospital</field>
        <field name="code">res.company</field>
        <field name="prefix">HOS/</field>
        <field name="padding">2</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="hos_person_sequence" model="ir.sequence">
        <field name="name">Hospital Person</field>
        <field name="code">hos.person</field>
        <field name="prefix">HP/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="hr_employee_sequence" model="ir.sequence">
        <field name="name">HR Employee</field>
        <field name="code">hr.employee</field>
        <field name="prefix">HP/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

</odoo>