<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="time_configuration_in_grace_time" model="time.configuration">
        <field name="name">IN Grace Time</field>
        <field name="code">100</field>
        <field name="value">60</field>
        <field name="progress">confirmed</field>
    </record>

    <record id="time_configuration_out_grace_time" model="time.configuration">
        <field name="name">OUT Grace Time</field>
        <field name="code">101</field>
        <field name="value">60</field>
        <field name="progress">confirmed</field>
    </record>

    <record id="time_configuration_full_day" model="time.configuration">
        <field name="name">Full Day</field>
        <field name="code">102</field>
        <field name="value">8</field>
        <field name="progress">confirmed</field>
    </record>

    <record id="time_configuration_half_day" model="time.configuration">
        <field name="name">Half Day</field>
        <field name="code">103</field>
        <field name="value">4</field>
        <field name="progress">confirmed</field>
    </record>

    <record id="monthly_leave_credit" model="leave.account">
        <field name="name">Monthly Leave Credit</field>
        <field name="code">103</field>
    </record>

    <record id="monthly_leave_lop" model="leave.account">
        <field name="name">Monthly Leave LOP</field>
        <field name="code">104</field>
    </record>

    <record id="monthly_leave_debit" model="leave.account">
        <field name="name">Monthly Leave Debit</field>
        <field name="code">105</field>
    </record>

    <record id="leave_journal_sequence" model="ir.sequence">
        <field name="name">Leave Journal</field>
        <field name="code">leave.journal</field>
        <field name="prefix">LJ/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="leave_item_sequence" model="ir.sequence">
        <field name="name">Leave Item</field>
        <field name="code">leave.item</field>
        <field name="prefix">LI/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="leave_account_sequence" model="ir.sequence">
        <field name="name">Leave Account</field>
        <field name="code">leave.account</field>
        <field name="prefix">LA/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>


</odoo>