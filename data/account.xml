<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="account_account_sequence" model="ir.sequence">
        <field name="name">Hospital Account</field>
        <field name="code">hos.account</field>
        <field name="prefix">HA/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="hos_assert_sequence" model="ir.sequence">
        <field name="name">Hos Assert</field>
        <field name="code">hos.assert</field>
        <field name="prefix">HAA/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="journal_entry_sequence" model="ir.sequence">
        <field name="name">Journal Entry</field>
        <field name="code">journal.entry</field>
        <field name="prefix">JE/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="journal_item_sequence" model="ir.sequence">
        <field name="name">Journal Item</field>
        <field name="code">journal.item</field>
        <field name="prefix">JI/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="hos_voucher_sequence" model="ir.sequence">
        <field name="name">Hos Voucher</field>
        <field name="code">hos.voucher</field>
        <field name="prefix">HV/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <!--Journal-->
    <record id="sales_journal" model="hos.journal">
        <field name="name">Sales</field>
        <field name="code">SJ</field>
    </record>

    <record id="sales_return_journal" model="hos.journal">
        <field name="name">Sales Return</field>
        <field name="code">SRJ</field>
    </record>

    <record id="purchase_journal" model="hos.journal">
        <field name="name">Purchase</field>
        <field name="code">PJ</field>
    </record>

    <record id="purchase_return_journal" model="hos.journal">
        <field name="name">Purchase Return</field>
        <field name="code">PRJ</field>
    </record>

    <record id="cash_receipts_journal" model="hos.journal">
        <field name="name">Cash Receipts</field>
        <field name="code">CRJ</field>
    </record>

    <record id="cash_payments_journal" model="hos.journal">
        <field name="name">Cash Payments</field>
        <field name="code">CPJ</field>
    </record>

    <record id="general_journal" model="hos.journal">
        <field name="name">General</field>
        <field name="code">GJ</field>
    </record>


</odoo>