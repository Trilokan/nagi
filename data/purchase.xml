<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="purchase_indent_sequence" model="ir.sequence">
        <field name="name">Purchase Indent</field>
        <field name="code">purchase.indent</field>
        <field name="prefix">PI/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="purchase_quotation_sequence" model="ir.sequence">
        <field name="name">Purchase Quotation</field>
        <field name="code">purchase.quotation</field>
        <field name="prefix">QR/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="hos_purchase_invoice_sequence" model="ir.sequence">
        <field name="name">Hospital Purchase Invoice</field>
        <field name="code">hos.purchase.invoice</field>
        <field name="prefix">INV/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="material_receipt_sequence" model="ir.sequence">
        <field name="name">Material Receipt</field>
        <field name="code">material.receipt</field>
        <field name="prefix">MR/</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>


</odoo>